{
  "name": "react-schema-form",
  "version": "0.3.1",
  "description": "React json schema form",
  "keywords": [
    "json schema",
    "schema form",
    "react",
    "react-component",
    "form"
  ],
  "author": "Steve Hu, Nicholas Azar",
  "license": "MIT",
  "main": "./lib/index",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "css",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/networknt/react-schema-form.git"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.16.6",
    "material-ui": "^0.16.1",
    "object-assign": "^4.0.1",
    "objectpath": "^1.2.1",
    "react": "^15.3.2",
    "react-dom": "^15.0.0",
    "react-tap-event-plugin": "^1.0.0",
    "tv4": "^1.2.7",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-runtime": "^6.18.0",
    "brace": "^0.9.0",
    "chai": "^3.2.0",
    "css-loader": "^0.25.0",
    "d3": "^4.3.0",
    "eslint": "^3.9.1",
    "eslint-plugin-react": "^6.5.0",
    "flux": "^3.1.0",
    "jest-cli": "^16.0.2",
    "jquery": "^3.1.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "react-ace": "4.0.0",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-schema-form-rc-select": "^0.2.0",
    "react-select": "^1.0.0-rc.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "scripts": {
    "lint": "eslint src",
    "preversion": "webpack -p && git add dist",
    "prepublish": "npm run snyk-protect && rimraf lib && babel src -d lib",
    "start": "webpack-dev-server -d --config example/webpack.config.js",
    "test": "npm run lint && jest",
    "snyk-protect": "snyk protect"
  },
  "jest": {
    "testPathDirs": [
      "src"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/lodash",
      "<rootDir>/node_modules/lodash-es"
    ]
  },
  "snyk": true
}
